<!--
  This is a TEI Site Index template that can be referenced to create more
  similar site indexes.

  Based off of the Digital Mitford Site Index structure:
  https://digitalmitford.org/si.xml
-->
<TEI>
  <teiHeader>
    <fileDesc>
      <titleStmt>
        <title></title>
        <author></author>
        <respStmt>
          <resp>Initial contents of TEI index generated by</resp>
          <name ref="https://github.com/fechan/lestrade-tei-tagger">Lestrade TEI Tagger</name>
        </respStmt>
        <sponsor></sponsor>
      </titleStmt>
      <publicationStmt>
        <authority></authority>
        <licence></licence>
      </publicationStmt>
    </fileDesc>
    <geoDecl xml:id="ITRF00" datum="ITRF00">International Terrestrial Reference Frame 2000</geoDecl>
  </teiHeader>

  <text>
    <body>
      <div type="people">
        <listPerson type="Person"></listPerson>
      </div>

      <div type="places">
        <listPlace type="Museum"></listPlace>
        <listPlace type="HistoricalSite"></listPlace>
        <listPlace type="Building"></listPlace>
        <listPlace type="City"></listPlace>
        <listPlace type="Country"></listPlace>
        <listPlace type="River"></listPlace>
      </div>

      <div type="art">
        <list type="Artwork">
          <figure xml:id="VARIABLE ArtId">
            <bibl>
              <title level="m">VARIABLE title</title>
              <author>
                <persName ref="VARIABLE #personId">VARIABLE artist</persName>
              </author>
              <date when="VARIABLE 1900-01-01">VARIABLE 1 January 1900</date>
              <note>VARIABLE Notable information about the art</note>
            </bibl>
          </figure>
        </list>
      </div>
      
      <div type="organizations">
        <listOrg type="Company"></listOrg>
      </div>

      <div type="ships">
        <listObject type="Ship">
          <object xml:id="VARIABLE objectId">
            <objectName>VARIABLE Object Name</objectName>
            <note>VARIABLE Notable Information about the ship</note>
          </object>
        </listObject>
      </div>
    </body>
  </text>
</TEI>